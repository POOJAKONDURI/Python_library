    "import numpy as np\n",
    "\n",
    "arr1 = np.array([0, 10, 20, 40, 60, 80])\n",
    "arr2 = np.array([10, 30, 40, 50, 70])\n",
    "\n",
    "exclusive_or = np.setxor1d(arr1, arr2)\n",
    "\n",
    "print(\"Unique values that are in only one (not both) of the input arrays:\")\n",
    "print(exclusive_or)\n"
   ]
  }
